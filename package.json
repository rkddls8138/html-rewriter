{
  "name": "html-rewriter-seo-nextjs",
  "version": "0.1.2",
  "description": "Next.js SDK for HTML Rewriter - SEO optimization for CSR applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkddls8138/html-rewriter.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
