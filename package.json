      "types": "./dist/middleware.d.ts"
    }
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "html-rewriter-seo-core": "github:rkddls8138/html-rewriter#pkg/core"
  },
  "keywords": [
    "nextjs",
    "seo",
    "meta-tags",
