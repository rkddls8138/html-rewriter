{
  "name": "@rkddls8138/seo-nextjs",
  "version": "0.1.1",
  "description": "Next.js SDK for HTML Rewriter - SEO optimization for CSR applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkddls8138/html-rewriter.git",
    "directory": "packages/sdk-nextjs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./middleware": {
      "require": "./dist/middleware.js",
      "import": "./dist/middleware.mjs",
      "types": "./dist/middleware.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/middleware.ts src/provider.tsx src/metadata.ts src/SeoHead.tsx --format cjs,esm --dts --external react --external next",
    "dev": "tsup src/index.ts src/middleware.ts src/provider.tsx src/metadata.ts src/SeoHead.tsx --format cjs,esm --dts --external react --external next --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@rkddls8138/seo-core": "^0.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "nextjs",
    "seo",
    "meta-tags",
    "html-rewriter",
    "csr",
    "ssr"
  ],
  "author": "",
  "license": "MIT"
}
