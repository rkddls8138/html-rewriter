{
  "name": "@rkddls8138/seo-react",
  "version": "0.1.0",
  "description": "React SDK for SEO Meta Tag Management - Hooks and Context Provider",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkddls8138/html-rewriter.git",
    "directory": "packages/react"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@rkddls8138/seo-core": "^0.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "react",
    "seo",
    "meta-tags",
    "hooks"
  ],
  "author": "",
  "license": "MIT"
}
